  <JScript><![CDATA[function Pattern_아웃룩으로_메일_검색하기_Headless() {

    this.mToday = new Date();
    this.mMailDir;
    this.mFirstDayStr;
    this.mLastDayStr;
    this.mMailItem;
    this.mMailSubject;
    this.mMails;

    this.run = function() {

        //------------------------------------------------
        STEP('Sc072adfbbbcf41c08f0a82bfdd2fb771', '[1] ExecuteScript');
        this.mToday= new Date();
        this.mMailDir = 'c:\\tempmail\\';
        RESULT = ""

        //------------------------------------------------
        STEP('S28832ca2e3fb4c3fa9e974045571c751', '[2] CreateDirectory');
        StepPlayer.Run('Desktop.CreateDirectory', this.createdirectory_info_2);

        //------------------------------------------------
        STEP('S90f32bc9e2744f5e8d7f9d026e23e1ab', '[3] DateTime_DateFormat');
        RESULT = Script.DataConverter.DateTime_DateFormat(null, , );

        //------------------------------------------------
        STEP('Sf6f379d6d4664715814cba5ab2cc78ba', '[4] DateTime_GetLastDateOfMonth');
        RESULT = Script.DataConverter.DateTime_GetLastDateOfMonth(null, );

        //------------------------------------------------
        STEP('S11cb2192bae8458297c918e5a11a5bfe', '[5] DateTime_DateFormat');
        RESULT = Script.DataConverter.DateTime_DateFormat(null, , );

        //------------------------------------------------
        STEP('S4bc79a1500cd43ef958da7604a21a23e', '[6] SyncMail');
        ActivityWorker.Play('Outlook', 'SyncMailActivity', this.activitylibraryitem_info_6);

        //------------------------------------------------
        STEP('S1c1e5a03768b462b94419b5eb0abd34a', '[7] GetMail');
        RESULT = ActivityWorker.Play('Outlook', 'GetMailActivity', this.activitylibraryitem_info_7);

        //------------------------------------------------
        STEP('Scf7d5d4f56aa48669b985b38bc02ef29', '[8] Foreach');
        for( of ) {

            //------------------------------------------------
            STEP('S937f3eb2cce8460c8b5d8e4a32cbbe1d', '[9] GetMailItem');
            RESULT = ActivityWorker.Play('Outlook', 'GetMailItemActivity', this.activitylibraryitem_info_9);

            //------------------------------------------------
            STEP('S9e81631051c1418fabf92bcc89cc4bc6', '[10] ExecuteScript');
            const reg = /[:*?"<>|]/g;

            RESULT += this.mMailSubject + '\n\n';
            this.mMailSubject = this.mMailSubject.replace( reg, '_' );


            //------------------------------------------------
            STEP('S34055386dd7b4727b757edf8db0ba0d6', '[11] String_ReplaceAll');
            this.mMailSubject = Script.DataConverter.String_ReplaceAll(null, this.mMailSubject, '\\', '');

            //------------------------------------------------
            STEP('S658c6c545e184f1eaec3e62b4a8ed842', '[12] String_ReplaceAll');
            this.mMailSubject = Script.DataConverter.String_ReplaceAll(null, this.mMailSubject, '/', '');

            //------------------------------------------------
            STEP('S17ebe64f032b42f68505091dc30aaa88', '[13] ExportMail');
            ActivityWorker.Play('Outlook', 'ExportMailActivity', this.activitylibraryitem_info_13);

        //------------------------------------------------
        }
        STEP('Sa4beb990699a49c096da9f934a7b8650', '[14] End-Foreach');

        //------------------------------------------------
        STEP('S7b98b5dd796e455d8cc84bd6e7e2605d', '[15] MessageBox');
        MsgBox('Total: ' + this.mMails.Count + '\n' + RESULT);


    }

    this.execute = function() {
        var result = null;
        result = this.run()
        if (result != null)
            return result;
    }
}
]]></JScript>
