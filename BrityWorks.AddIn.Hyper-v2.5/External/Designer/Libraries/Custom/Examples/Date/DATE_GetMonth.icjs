if(Script.JavaScript == undefined || Script.JavaScript == null) {
	Script.JavaScript = new Object();
}

if(Script.JavaScript.DATE_GetMonth == null || Script.JavaScript.DATE_GetMonth == undefined) {
	Script.JavaScript.DATE_GetMonth = function (date) {
	//Body Begin
		var varDate = new Date(date);
		return varDate.getMonth();
	//Body End
	}
