if(Script.JavaScript == undefined || Script.JavaScript == null) {
	Script.JavaScript = new Object();
}

if(Script.JavaScript.STRING_IsSpaceChar == null || Script.JavaScript.STRING_IsSpaceChar == undefined) {
	Script.JavaScript.STRING_IsSpaceChar = function (inputString) {
	//Body Begin
		if (inputString == " " || inputString == "\t") {
		    return true;
		}
		return false;
	//Body End
	}
}