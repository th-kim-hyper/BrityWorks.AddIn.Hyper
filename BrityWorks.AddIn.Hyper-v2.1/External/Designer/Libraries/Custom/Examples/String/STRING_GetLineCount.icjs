	Script.JavaScript = new Object();
}

if(Script.JavaScript.STRING_GetLineCount == null || Script.JavaScript.STRING_GetLineCount == undefined) {
	Script.JavaScript.STRING_GetLineCount = function (inputString) {
	//Body Begin
